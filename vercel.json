{
  "version": 2,
  "buildCommand": "cd apps/web-app && npm install && npm run build",
  "outputDirectory": "apps/web-app/dist",
  "installCommand": "npm install",
  "framework": null,
  "ignoreCommand": "git diff HEAD^ HEAD --quiet .",
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}